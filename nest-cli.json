      "sourceRoot": "apps/smart-billboard/src",
      "compilerOptions": {
        "tsConfigPath": "apps/smart-billboard/tsconfig.app.json"
      }
    },
    "billboard-iot": {
      "type": "application",
      "root": "apps/billboard-iot",
      "entryFile": "main",
      "sourceRoot": "apps/billboard-iot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billboard-iot/tsconfig.app.json"
      }
    }
  }
}