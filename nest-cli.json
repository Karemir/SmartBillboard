{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/smart-billboard/src",
  "monorepo": true,
  "root": "apps/smart-billboard",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/smart-billboard/tsconfig.app.json"
  },
  "projects": {
    "smart-billboard": {
      "type": "application",
      "root": "apps/smart-billboard",
